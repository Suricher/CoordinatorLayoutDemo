            android:name=".exmcollapsing.ImageFadeActivity"
            android:theme="@style/AppCompatTheme"/>
        <activity
            android:name=".exmcollapsing.ScrollFlagActivity"
            android:theme="@style/AppCompatTheme"/>
    </application>

</manifest>