        </activity>

        <activity
            android:name=".exmcollapsing.AlipayActivity"
            android:theme="@style/AppCompatTheme"/>
        <activity
            android:name=".exmcollapsing.CollapsePinActivity"
            android:theme="@style/AppCompatTheme"/>
        <activity
            android:name=".exmcollapsing.CollapseParallaxActivity"
            android:theme="@style/AppCompatTheme"/>
